{
  "version": 2,
  "builds": [
    {
      "src": "COMEDORCOBRO/build/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "COMEDORUSUARIO/build/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "BACKEND/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "COMEDORCOBRO/build/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "BACKEND/$1"
    }
  ]
}
